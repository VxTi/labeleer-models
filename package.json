{
  "name": "@labeleer/models",
  "version": "1.0.3",
  "private": false,
  "type": "module",
  "description": "Utility parser methods used by all Labeleer tools",
  "author": "Luca Warmenhoven",
  "license": "ISC",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build": "npm run clean; esbuild ./src/index.ts --bundle --packages=external --minify --platform=node --format=esm --outfile=./dist/index.js && npm run build:types",
    "prepublishOnly": "npm run build",
    "test": "vitest run -u",
    "lint": "eslint --fix \"./src/**/*.ts\" --cache --max-warnings 0",
    "sanity": "npm run lint; npm run test; tsc --noEmit; npm run clean",
    "clean": "rm -rf ./dist tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/gettext-parser": "^8.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.2",
    "gettext-parser": "^8.0.0",
    "lodash": "^4.17.21",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  }
}
